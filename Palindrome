Palindrome

cnt = [4,4,4,9,4,7,1,3,7]
    
res = ''
for c in '9876543210':
    if not res and c == '0':
        continue
    print(c)
    print(cnt[c])
    for x  in range(int(cnt[a]) // 2):
        res += c
    
mid = ''
for c in '9876543210':
    if cnt[c] % 2 :
        mid = c
        break
print(res + mid + res[::-1])


